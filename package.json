{
  "name": "revoa-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 500",
    "preview": "vite preview",
    "prebuild": "npm run lint",
    "postbuild": "cp _redirects dist/_redirects",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "server": "node src/server/index.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset --yes",
    "supabase:push": "supabase db push",
    "supabase:types": "supabase gen types typescript --local > src/types/database.ts"
  },
  "dependencies": {
    "@netlify/functions": "^3.1.2",
    "@shopify/app-bridge": "^3.7.9",
    "@shopify/app-bridge-react": "^4.0.0",
    "@shopify/app-bridge-utils": "^3.5.1",
    "@shopify/polaris": "^12.10.0",
    "@shopify/shopify-api": "^9.3.1",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.28.0",
    "@tanstack/react-table": "^8.13.2",
    "@tanstack/react-virtual": "^3.1.3",
    "@typeform/embed-react": "^3.10.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "express": "^4.21.2",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "helmet": "^7.1.0",
    "js-cookie": "^3.0.5",
    "libsodium-wrappers": "^0.7.15",
    "lucide-react": "^0.344.0",
    "node-fetch": "^2.7.0",
    "pdfjs-dist": "^4.0.379",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.4",
    "react-intersection-observer": "^9.8.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "socket.io-client": "^4.7.5",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@shopify/app": "^3.58.2",
    "@shopify/cli": "^3.76.2",
    "@shopify/cli-kit": "^3.76.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-cookie": "^3.0.6",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "supabase": "^1.145.4",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.5.0",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
